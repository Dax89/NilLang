word /string  # s n a -- s n
    dup rot swap -
    rot rot +
    swap
end

word nl 
    10 emit
end

word say # s n --
    begin
        dup 0 == if break end
        over c@ emit
        1 /string
    repeat
end

word 2@ ( v )
    v @
    v cell + @ 
    swap
end

word 2! ( a b v )
    b v ! 
    a v cell + !
end
