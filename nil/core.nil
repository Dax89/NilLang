word /string  # s n a -- s n
    dup rot swap -
    rot rot +
    swap
end

word nl 
    10 emit
end

word hex 16 base ! end
word dec 10 base ! end
word oct 8 base ! end
word bin 2 base ! end

word say # s n --
    begin
        over c@ emit
        1 /string
        dup 0 == if break end
    repeat

    drop drop
end

word 2@ ( v )
    v @
    v cell + @ 
    swap
end

word 2! ( a b v )
    b v ! 
    a v cell + !
end
